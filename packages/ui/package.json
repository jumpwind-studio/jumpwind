{
  "name": "@jumpwind/ui",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "private": true,
  "exports": {
    "./*": "./registry/jumpwind/ui/*.tsx",
    ".": "./src/index.ts",
    "./src/*": "./src/*.ts",
    "./internal/*": null,
    "./package.json": "./package.json",
    "./registry.json": "./registry.json",
    "./lib/*": "./registry/jumpwind/lib/*.ts",
    "./utils": "./registry/jumpwind/lib/utils.ts"
  },
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "biome check",
    "format:fix": "biome check . --fix",
    "test": "bun run --bun vitest",
    "typecheck": "tsc --noEmit",
    "registry:build": "shadcn registry:build && bun run format:fix",
    "registry:script": "bun run ./scripts/build-registry.ts",
    "storybook:build": "storybook build",
    "storybook:dev": "storybook dev -p 6006",
    "storybook": "storybook:dev"
  },
  "dependencies": {
    "@corvu/utils": "^0.4.2",
    "@effect/platform": "^0.90.7",
    "@effect/platform-bun": "^0.79.0",
    "@kobalte/core": "^0.13.11",
    "@tanstack/solid-form": "^1.19.3",
    "@unpic/solid": "^1.0.0",
    "effect": "catalog:effect",
    "corvu": "^0.7.2",
    "lucide-solid": "^0.534.0",
    "shadcn": "^3.2.1",
    "solid-js": "^1.9.9",
    "solid-sonner": "^0.2.8",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.0",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8"
  },
  "devDependencies": {
    "typescript": "^5.9.2",
    "storybook": "^9.1.5",
    "storybook-solidjs-vite": "^9.0.3",
    "@chromatic-com/storybook": "^4.1.1",
    "@storybook/addon-onboarding": "^9.1.5",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-a11y": "^9.1.5",
    "@storybook/addon-links": "^9.1.5",
    "@storybook/addon-vitest": "^9.1.5",
    "@vitest/coverage-v8": "^3.2.4",
    "playwright": "^1.55.0",
    "vitest": "^3.2.4",
    "@vitest/browser": "^3.2.4"
  }
}
