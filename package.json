{
  "name": "jumpwind",
  "type": "module",
  "private": true,
  "description": "Monorepo for all things Jumpwind",
  "scripts": {
    "build": "turbo run build",
    "check": "biome check .",
    "check:fix": "biome check . --fix",
    "circular": "madge --extensions ts --circular --no-color --no-spinner packages/*",
    "circular:all": "madge --extensions ts,tsx --circular --no-color --no-spinner packages/* apps/*",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo run clean",
    "dev": "turbo run dev",
    "format": "biome format .",
    "format:fix": "biome format . --fix",
    "lint": "biome lint .",
    "lint:circular": "madge --circular .",
    "lint:fix": "biome lint . --fix",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "typecheck": "turbo run typecheck --ui=stream"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "typescript": "^5.9.2"
    },
    "catalogs": {
      "effect": {
        "effect": "3.17.13",
        "@effect/platform": "^0.90.7",
        "@effect/platform-bun": "^0.79.0"
      },
      "frontend": {
        "@corvu/utils": "^0.4.2",
        "@effect/vitest": "^0.25.1",
        "@kobalte/core": "^0.13.11",
        "@solid-primitives/rootless": "^1.5.2",
        "@solid-primitives/keyboard": "^1.3.3",
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/vite": "^4.1.13",
        "@tanstack/solid-form": "^1.19.3",
        "@unpic/solid": "^1.0.0",
        "@vitest/browser": "^3.2.4",
        "@vitest/coverage-v8": "^3.2.4",
        "cmdk-solid": "^1.1.2",
        "cookie-es": "^2.0.0",
        "corvu": "^0.7.2",
        "lucide-solid": "^0.534.0",
        "playwright": "^1.55.0",
        "shadcn": "^3.2.1",
        "solid-devtools": "^0.34.3",
        "solid-js": "^1.9.9",
        "solid-persistent": "^0.1.0",
        "solid-sonner": "^0.2.8",
        "tailwind-merge": "^3.3.1",
        "tailwind-variants": "^3.1.0",
        "tailwindcss": "^4.1.13",
        "tw-animate-css": "^1.3.8",
        "vite": "^7.1.4",
        "vite-plugin-solid": "^2.11.8",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4"
      }
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.3",
    "@turbo/gen": "^2.5.6",
    "@types/bun": "^1.2.21",
    "madge": "^8.0.0",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "packageManager": "bun@1.2.21"
}
